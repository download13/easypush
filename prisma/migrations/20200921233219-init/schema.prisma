generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "***"
}

model BrowserHandle {
  id            Int            @id @default(autoincrement())
  subscription  String
  enabled       Boolean        @default(true)
}

model Channel {
  id            Int            @id @default(autoincrement())
  handleId      Int
  handle        BrowserHandle  @relation(fields: [handleId], references: [id])
  label         String
}
